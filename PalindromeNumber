public class PalindromeNumber {

	public static void main(String[] args) {

          int n=121;
          
          Boolean isPalindrome= checkPalindorme(n);
          
          System.out.print(isPalindrome);

	}

	private static Boolean checkPalindorme(int n) {
         
		int reverse=0;
		
		int original=n;
		
		while(n>0) {
			
			int digit=n%10;
			
			reverse=(reverse*10)+digit;
			
			n/=10;
			
		}
		
		if(reverse==original) {
			return true;
		}
		
		return false;


	}

}
